{
    "name": "tsmsogn/cakephp-state-machine",
    "description": "StateMachineBehavior for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "state", "machine"],
    "homepage": "https://github.com/tsmsogn/cakephp-state-machine",
    "license": "MIT",
    "authors": [
        {
            "name": "David Steinsland",
            "role": "Author"
        },
        {
            "name": "tsmsogn",
            "email": "tsmsogn@gmail.com"
        }
    ],
    "require": {
        "cakephp/cakephp": "^3.4",
        "composer/installers": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
